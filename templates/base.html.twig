<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
    {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
    {% block stylesheets %}
      <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css">
      <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-tagsinput@0.7.1/dist/bootstrap-tagsinput.min.css">
      <link href="/bundles/siganushkamedia/main.css" rel="stylesheet">
    {% endblock %}

    {% block javascripts %}
      <script src="https://cdn.jsdelivr.net/npm/jquery@3.7.1/dist/jquery.min.js"></script>
      <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
      <script src="https://cdn.jsdelivr.net/npm/bootstrap-tagsinput@0.7.1/dist/bootstrap-tagsinput.min.js"></script>
      <script src="/bundles/siganushkamedia/main.js"></script>
      <script src="/bundles/siganushkaproduct/main.js"></script>
      <script src="/bundles/siganushkaregion/main.js"></script>
    {% endblock %}
  </head>
  <body>
  <div class="container-fluid my-4">
    <div class="row">
      <div class="col">
      <ul class="dropdown-menu position-static d-grid gap-1 p-2 rounded-3 mx-0 w-220px" data-bs-theme="light">
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_index_index') }}">Index</a>
        </li>
        <li>
          <h6 class="dropdown-header">siganushka/region-bundle</h6>
        </li>
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_region_regiontype') }}">RegionType</a>
        </li>
        <li>
          <h6 class="dropdown-header">siganushka/media-bundle</h6>
        </li>
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_media_index') }}">media</a>
        </li>
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_media_mediauploadtype') }}">MediaUploadType</a>
        </li>
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_media_mediatype') }}">MediaType</a>
        </li>
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_media_mediachanneltype') }}">MediaChannelType</a>
        </li>
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_media_mediafiletype') }}">MediaFileType</a>
        </li>
        <li>
          <h6 class="dropdown-header">siganushka/product-bundle</h6>
        </li>
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_product_index') }}">products</a>
        </li>
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_product_producttype') }}">ProductType</a>
        </li>
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_product_productoptiontype') }}">ProductOptionType</a>
        </li>
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_product_productoptionvaluetype') }}">ProductOptionValueType</a>
        </li>
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_product_productvariantcollectiontype') }}">ProductVariantCollectionType</a>
        </li>
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_product_productvarianttype') }}">ProductVariantType</a>
        </li>
        <li>
          <h6 class="dropdown-header">siganushka/order-bundle</h6>
        </li>
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_order_index') }}">orders</a>
        </li>
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_order_ordertype') }}">OrderType</a>
        </li>
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_order_orderitemtype') }}">OrderItemType</a>
        </li>
        <li>
          <h6 class="dropdown-header">siganushka/banner-bundle</h6>
        </li>
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_banner_index') }}">banners</a>
        </li>
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_banner_bannertype') }}">BannerType</a>
        </li>
      </ul>
      </div>
      <div class="col-9">
        {% for label, messages in app.flashes %}
          {% for message in messages %}
            <div class="alert alert-{{ label }}">{{ message }}</div>
          {% endfor %}
        {% endfor %}
        {% block body %}{% endblock %}
      </div>
    </div>
  </body>
</html>
