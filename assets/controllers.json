{
    "controllers": {
        "@siganushka/generic-bundle": {
            "collection": {
                "enabled": true,
                "fetch": "lazy"
            }
        },
        "@siganushka/media-bundle": {
            "media": {
                "enabled": true,
                "fetch": "lazy",
                "autoimport": {
                    "@siganushka/media-bundle/dist/bootstrap5.css": true
                }
            }
        },
        "@siganushka/region-bundle": {
            "region": {
                "enabled": true,
                "fetch": "lazy"
            }
        },
        "@symfony/ux-autocomplete": {
            "autocomplete": {
                "enabled": true,
                "fetch": "eager",
                "autoimport": {
                    "tom-select/dist/css/tom-select.default.css": false,
                    "tom-select/dist/css/tom-select.bootstrap4.css": false,
                    "tom-select/dist/css/tom-select.bootstrap5.css": true
                }
            }
        }
    },
    "entrypoints": []
}
